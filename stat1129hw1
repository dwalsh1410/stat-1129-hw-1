mydict = {"sunny":"play", "Rainy":"watchTV", "Cloudy":"Walk" } #For Problem 3a. 
print(type(mydict))
print(mydict)

mydict = {"sunny":"play", "Rainy":"watchTV", "Cloudy":"Walk" } #For Problem 3b. 
print(type(mydict))
print(mydict)
mydict.update({"snowy":"ski"})
print(mydict)
