nums= list(range(30,60,5)) #For problem a. 
print(nums)

nums= list(range(30,60,5)) #For problem b. 
print(nums[::-1])

nums= list(range(30,70,5)) #For problem c. 
print(nums[::-1])

list = []                   # For problem 2a. 
for i in range(-1, 20, 1):
    list.append(i+1)
print(list)

list = []                  #For problem 2b. 
for i in range(-1,20,1):
    list.append(i+1)
list.remove(0)
print(list)

list = []               #For problem 2c. 
for i in range(-1,20,1):
    list.append(i+1)
list.remove(0)
print(list)
min(list)

